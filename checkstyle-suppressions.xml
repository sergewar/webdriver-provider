<?xml version="1.0"?>
<!DOCTYPE suppressions PUBLIC
        "-//Puppy Crawl//DTD Suppressions 1.0//EN"
        "http://www.puppycrawl.com/dtds/suppressions_1_0.dtd">
<suppressions>
    <suppress checks="HideUtilityClassConstructor" files=".*.java"/>
    <suppress checks="ParenPad" files=".*.java"/>
    <suppress checks="JavadocType" files=".*.java"/>
    <suppress checks="AvoidStarImport" files=".*.java"/>
    <suppress checks="LocalFinalVariableName" files=".*.java"/>
    <suppress checks="MemberName" files=".*.java"/>
</suppressions>
